{
  "nodeName": "quickbooks online",
  "summary": "此節點用於使用 QuickBooks Online API。",
  "parameters": [
    {
      "name": "resource",
      "type": "options",
      "description": "要操作的資源類型，如帳單、客戶、員工等。",
    },
    {
      "name": "operation",
      "type": "options",
      "description": "要執行的操作，如創建、刪除、獲取資料等。",
    },
    {
      "name": "vendorref",
      "type": "options",
      "description": "帳單所屬的供應商的 ID。",
    },
    {
      "name": "line",
      "type": "collection",
      "description": "交易的單個行項目。",
    },
    {
      "name": "additionalfields",
      "type": "collection",
      "description": "其他可選的欄位。",
    }
  ],
  "example": {
    "node": {
      "parameters": {
        "resource": "bill",
        "operation": "get",
        "vendorref": "12345",
        "line": [
          {
            "accountId": "6789",
            "Amount": 100,
            "Description": "Item description",
            "DetailType": "ItemBasedExpenseLineDetail",
            "itemId": "54321",
            "LineNum": 1
          }
        ],
        "additionalFields": {}
      }
    }
  }
}