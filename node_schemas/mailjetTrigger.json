{
  "name": "n8n-nodes-base.mailjetTrigger",
  "displayName": "Mailjet Trigger",
  "description": "Handle Mailjet events via webhooks",
  "properties": [
    {
      "displayName": "Event",
      "name": "event",
      "type": "options",
      "required": true,
      "default": "open",
      "options": [
        {
          "name": "email.blocked",
          "value": "blocked"
        },
        {
          "name": "email.bounce",
          "value": "bounce"
        },
        {
          "name": "email.open",
          "value": "open"
        },
        {
          "name": "email.sent",
          "value": "sent"
        },
        {
          "name": "email.spam",
          "value": "spam"
        },
        {
          "name": "email.unsub",
          "value": "unsub"
        }
      ],
      "description": "Determines which resource events the webhook is triggered for"
    }
  ]
}